-+					     proto.pppoe.padt_work);
-+	struct sock *sk = sk_pppox(po);
-+
-+	lock_sock(sk);
-+	pppox_unbind_sock(sk);
-+	release_sock(sk);
-+	sock_put(sk);
-+}
